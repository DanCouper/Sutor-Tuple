{
  "name": "sutor-tuple",
  "version": "0.0.1",
  "description": "A tiny tuple implementation.",
  "main": "src/tuple.js",
  "scripts": {
    "ava": "ava test",
    "clean": "rm -rf coverage && rm -rf dist",
    "compile": "babel src -d dist -s",
    "coverage": "nyc --reporter=lcov npm run ava && nyc report",
    "coveralls": "nyc npm run ava && nyc report --reporter=text-lcov | coveralls && rm -rf ./coverage",
    "lint": "eslint src/**/*.js",
    "test": "npm run lint && ava test --tap | faucet"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanCouper/Sutor-Tuple"
  },
  "keywords": [
    "es6",
    "es2015",
    "babel",
    "tuple"
  ],
  "author": "Daniel Couper <danielcouper81@googlemail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.9.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "faucet": "0.0.1",
    "nyc": "^5.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "parser": "babel-eslint",
    "rules": {
      "eol-last": [
        0
      ],
      "no-mixed-requires": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "quotes": [
        2,
        "single"
      ],
      "strict": 0
    }
  }
}
